<div class=container>
	<h1>Vehicles List</h1>
	<%= form_tag vehicles_path, :method => 'get' do %>
		<%= text_field_tag :search %>
		<%= submit_tag "Search", name: nil  %>
	<% end %>
	<hr>

</div>
<% @vehicles.each do |vehicle| %>
		<div class="container">
			<h5><%= vehicle.year %> <%= vehicle.make %> <%= vehicle.model %>
				 <%= link_to "More", vehicle_path(vehicle), :class => 'btn btn-outline-info '%></h5>
				 <hr>
		</div>


	<% end %>